config_version=5

[application]

config/name="Megaman_X_Game"
run/main_scene="res://Scenes/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://project_icon.png"

[autoload]

BgmPlayer="*res://Scenes/Misc/background_audio_player.tscn"
Global="*res://Scenes/Misc/global.tscn"

[debug]

settings/stdout/print_fps=true
gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_width=1600
window/size/viewport_height=980
window/stretch/scale=2.0
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="Megaman_X_Game"

